{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:position:ex5-pos-trf-10",
  "$metadata" : {
    "domain" : "position",
    "schemaId" : "urn:a5x:ex5:schema:v1:position:ex5-pos-trf-10"
  },
  "title" : "Position Transfer Cancellation",
  "description" : "Message for position transfer Cancellation",
  "type" : "object",
  "properties" : {
    "positionTransferReference" : {
      "type" : "string",
      "maxLength" : 15,
      "minLength" : 1,
      "pattern" : "^[A-Z0-9]{1,15}$",
      "description" : "Position transfer reference identifier",
      "examples" : [ "PST123456789012", "TRANSFER001", "POS12345" ]
    },
    "participantIdentification" : {
      "$ref" : "../../objects/business/PartyId.json",
      "description" : "Provides details about the participant identification"
    }
  },
  "required" : [ "positionTransferReference", "participantIdentification" ],
  "additionalProperties" : false,
  "examples" : [ {
    "positionTransferReference" : "113624876548555",
    "participantIdentification" : {
      "id" : 999,
      "category" : 3
    }
  } ]
}