{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "urn:a5x:ex5:schema:v1:accounts_and_links_management:ex5-reg-rsp-08",
  "$metadata": {
    "domain": "accounts_and_links_management",
    "schemaId": "urn:a5x:ex5:schema:v1:accounts_and_links_management:ex5-reg-rsp-08"
  },
  "title": "Link Change Notification",
  "description": "Notification message for changes made to a link by screen interface, by message and mantenance response",
  "type": "object",
  "properties": {
    "participantIdentification": {
      "$ref": "../../objects/business/PartyId.json",
      "description": "Participant ID associated with the user"
    },
    "operationType": {
      "$ref": "../../objects/business/ExternalCodeList.json",
      "description": "Type of operation to perform",
      "$comment": "Consult External Code List table ExternalOperationTypeCode"
    },
    "transactionOrigin": {
      "$ref": "../../objects/business/ExternalCodeList.json",
      "description": "Type of operation to perform",
      "$comment": "Consult External Code List table ExternalTransactionOriginTypeCode"
    },
    "linkData": {
      "$ref": "../../objects/business/BaseRegistrationLink.json",
      "description": "Details of the changes made to the Link"
    },
    "statusSuccess": {
      "type": "boolean",
      "description": "Indicates if the operation was successful (true) or not (false)"
    },
    "statusError": {
      "$ref": "../../objects/business/RegisterStatus.json"
    }
  },
  "required": [
    "participantIdentification",
    "operationType",
    "transactionOrigin"
  ],
  "additionalProperties": false,
  "examples": [
    {
      "participantIdentification": {
        "id": 999,
        "category": 3
      },
      "operationType": 1,
      "transactionOrigin": 1,
      "linkData": {
        "linkType": 1,
        "linkStatus": 6,
        "participantOriginIdentification": {
          "id": 1001,
          "category": 2
        },
        "accountOriginIdentification": {
          "id": 5001
        },
        "participantDestinationIdentification": {
          "id": 1002,
          "category": 2
        },
        "accountDestinationIdentification": {
          "id": 5002
        },
        "managementGroup": 1,
        "positionConsolidation": true,
        "automaticAcceptance": true
      },
      "statusSuccess": true
    },
    {
      "participantIdentification": {
        "id": 999,
        "category": 3
      },
      "operationType": 1,
      "transactionOrigin": 1,
      "statusSuccess": false,
      "statusError": {
        "statusCode": 1,
        "errorIdentification": [
          {
            "code" : "TEC00000",
            "message": "Error X",
            "errorDescription": "COD12452"
          }
        ]
      }
    }
  ]
}