{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:SecuritiesCollateralProposal",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:SecuritiesCollateralProposal"
  },
  "title" : "Securities Collateral Proposal",
  "description" : "Provides details about the proposal.",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral identification as known by party. Its identification is generated by the requesting party"
    },
    "quantity" : {
      "$ref" : "../../types/DecimalNumber.json",
      "description" : "Quantity requested to collateral transfer"
    },
    "custodianInformationOrigin" : {
      "$ref" : "../../objects/business/CustodianInformation.json",
      "description" : "This block identifies the custodian Information"
    },
    "tickerSymbol" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Letters that identify stocks traded on stock exchange"
    },
    "centralDepositaryOperationNumber" : {
      "type" : "string",
      "pattern" : "^[0-9]{1,9}$",
      "description" : "Central Depository Operation Number"
    },
    "custodianInformationDestination" : {
      "$ref" : "../../objects/business/CustodianInformation.json",
      "description" : "This block identifies the custodian Information"
    }
  },
  "required" : [ "collateralIdentification", "quantity", "custodianInformationOrigin", "tickerSymbol", "centralDepositaryOperationNumber", "custodianInformationDestination" ],
  "additionalProperties" : false,
  "examples" : [ {
    "collateralIdentification" : "1",
    "quantity" : "1000.0",
    "custodianInformationOrigin" : {
      "participantInformation" : {
        "id" : 456,
        "category" : 2
      },
      "accountIdentification" : {
        "id" : 123456789
      },
      "subAccount" : "001"
    },
    "tickerSymbol" : "AAPL",
    "centralDepositaryOperationNumber" : "123456789",
    "custodianInformationDestination" : {
      "participantInformation" : {
        "id" : 456,
        "category" : 2
      },
      "accountIdentification" : {
        "id" : 123456789
      },
      "subAccount" : "001"
    }
  } ]
}