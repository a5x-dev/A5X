{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:ValidityDateTimeRange",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:ValidityDateTimeRange"
  },
  "title" : "Validity DateTime Range",
  "description" : "Defines a period of validity with start and end date/time",
  "type" : "object",
  "properties" : {
    "fromDateTime" : {
      "$ref" : "../../types/ISODateTime.json",
      "description" : "Date and time at which the range starts",
      "examples" : [ "2025-09-01T09:00:00Z", "2025-12-31T00:00:00Z" ]
    },
    "toDateTime" : {
      "$ref" : "../../types/ISODateTime.json",
      "description" : "Date and time at which the range ends",
      "examples" : [ "2025-09-30T17:00:00Z", "2026-01-01T23:59:59Z" ]
    }
  },
  "additionalProperties" : false,
  "required" : [ "fromDateTime", "toDateTime" ],
  "examples" : [ {
    "fromDateTime" : "2025-09-01T09:00:00Z",
    "toDateTime" : "2025-09-30T17:00:00Z"
  } ]
}