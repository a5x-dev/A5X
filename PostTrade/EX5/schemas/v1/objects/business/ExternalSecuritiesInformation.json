{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:ExternalSecuritiesInformation",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:ExternalSecuritiesInformation"
  },
  "title" : "External Securities Information",
  "description" : "This block provides information about external securities collateral valuation",
  "type" : "object",
  "properties" : {
    "collateralReference" : {
      "type" : "string",
      "maxLength" : 35,
      "description" : "Reference of the piece of valued collateral",
      "examples" : [ "EXT-SEC-REF-001", "EXTERNAL-789012" ]
    },
    "issueDate" : {
      "$ref" : "../../types/ISODate.json",
      "description" : "Provides issue date of collateral",
      "examples" : [ "2024-01-15", "2023-12-01" ]
    },
    "maturityDate" : {
      "$ref" : "../../types/ISODate.json",
      "description" : "Provides maturity date of collateral",
      "examples" : [ "2026-01-15", "2025-12-01" ]
    },
    "depositary" : {
      "$ref" : "../../objects/business/ExternalCodeList.json",
      "description" : "Central Depositary",
      "$comment" : "Consult External Code List table ExternalDepositaryCode",
      "examples" : [ 1, 2, 3 ]
    },
    "accountCentralDepositary" : {
      "type" : "integer",
      "description" : "Account in DTC / Euroclear",
      "examples" : [ 123456, 987654, 555000 ]
    },
    "fedAccount" : {
      "type" : "string",
      "maxLength" : 35,
      "description" : "FED (ABA) account. Conta no FED (ABA)",
      "examples" : [ "021000021", "026009593", "FED-ACC-123456" ]
    },
    "accountNameSpecialInstructions" : {
      "type" : "string",
      "maxLength" : 20,
      "description" : "Special instructions account's name. Nome da conta (Special Instructions)",
      "examples" : [ "SPECIAL-INST-001", "SI-ACCOUNT", "INST-12345" ]
    }
  },
  "required" : [ "collateralReference", "maturityDate" ],
  "additionalProperties" : false,
  "examples" : [ {
    "collateralReference" : "EXT-SEC-REF-001",
    "issueDate" : "2024-01-15",
    "maturityDate" : "2026-01-15",
    "depositary" : 2,
    "accountCentralDepositary" : 987654,
    "fedAccount" : "026009593",
    "accountNameSpecialInstructions" : "SI-ACCOUNT"
  } ]
}