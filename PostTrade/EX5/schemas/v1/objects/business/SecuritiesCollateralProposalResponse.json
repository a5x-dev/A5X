{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:SecuritiesCollateralProposalResponse",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:SecuritiesCollateralProposalResponse"
  },
  "title" : "Securities Collateral Proposal",
  "description" : "Provides details on the securities collateral proposal.",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral identification as known by party. Its identification is generated by the requesting party"
    },
    "assetNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identifies the register number of the collateral deposit assigned by the central counterparty"
    },
    "quantity" : {
      "$ref" : "../../types/DecimalNumber.json",
      "description" : "Quantity requested to collateral transfer"
    },
    "collateralValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Collateral value in active currency"
    },
    "statusInformation" : {
      "$ref" : "../../objects/business/CollateralStatus.json",
      "description" : "Overall status of the collateral transfer instruction"
    }
  },
  "required" : [ "collateralIdentification", "assetNumber", "quantity", "collateralValue", "statusInformation" ],
  "additionalProperties" : false,
  "examples" : [ {
    "collateralIdentification" : "1",
    "assetNumber" : "AAPL",
    "quantity" : "100.0",
    "collateralValue" : {
      "currency" : "USD",
      "amount" : "1000.0"
    },
    "statusInformation" : {
      "statusCode" : 1,
      "errorIdentification" : [ {
        "code" : "TEC00000",
        "message" : "Error X",
        "errorDescription" : "COD12452",
        "refId" : "REF12345"
      } ]
    }
  } ]
}