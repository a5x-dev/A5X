{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:FederalBondsResponse",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:FederalBondsResponse"
  },
  "title" : "Other Collateral Proposal Response",
  "description" : "Provides details on other collateral proposal.",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral ID given by the requestor participant"
    },
    "assetNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identifies the register number of the collateral deposit assigned by the central counterparty"
    },
    "quantity" : {
      "$ref" : "../../types/DecimalNumber.json",
      "description" : "Quantity requested to collateral transfer"
    },
    "collateralValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Collateral accepted value after taking into account the haircut."
    },
    "statusInformation" : {
      "$ref" : "../../objects/business/CollateralStatus.json",
      "description" : "Provides Status Information"
    }
  },
  "required" : [ "collateralIdentification", "assetNumber", "quantity", "collateralValue", "statusInformation" ],
  "additionalProperties" : true,
  "examples" : [ {
    "collateralIdentification" : "OTHER-001",
    "assetNumber" : "AAPL",
    "quantity" : "100.0",
    "collateralValue" : {
      "amount" : "24000.00",
      "currency" : "BRL"
    },
    "statusInformation" : {
      "statusCode" : 1,
      "errorIdentification" : [ {
        "code" : "TEC00000",
        "message" : "Error X",
        "errorDescription" : "COD12452",
        "refId" : "REF12345"
      } ]
    }
  } ]
}