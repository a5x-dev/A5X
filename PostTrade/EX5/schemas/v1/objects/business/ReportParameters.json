      "description" : "Frequency of the report",
      "$comment" : "Consult External Code List table EventFrequency6Code"
    },
    "reportNumber" : {
      "type" : "string",
      "pattern" : "^[0-9]{5}$",
      "description" : "Sequential number of the report"
    },
    "reportDateAndTime" : {
      "$ref" : "../../types/ISODateTime.json",
      "description" : "Reference date of the information"
    }
  },
  "required" : [ "frequency", "reportDateAndTime" ],
  "additionalProperties" : false,
  "examples" : [ {
    "frequency" : 2,
    "reportNumber" : "00132",
    "reportDateAndTime" : "2015-09-18"
  } ]
}