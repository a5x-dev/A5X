{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:FederalBondsTransfer",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:FederalBondsTransfer"
  },
  "title" : "Other Collateral Transfer",
  "description" : "Other collateral transfer item with collateral type and optional accounts",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral ID given by the requestor participant"
    },
    "collateralType" : {
      "$ref" : "../../objects/business/ExternalCodeList.json",
      "description" : "Provides details about the collateral type",
      "$comment" : "Consult External Code List table ExternalCollateralTypeCode"
    },
    "assetNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identifies the register number of the collateral deposit assigned by the central counterparty"
    },
    "assignerAccount" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Provides assigner account identification"
    },
    "assigneeAccount" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Provides the assignee account identification"
    },
    "quantity" : {
      "$ref" : "../../types/DecimalNumber.json",
      "description" : "Quantity requested to collateral transfer"
    },
    "collateralValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Collateral accepted value after taking into account the haircut"
    }
  },
  "required" : [ "collateralIdentification", "assetNumber" ],
  "additionalProperties" : false,
  "examples" : [ {
    "collateralIdentification" : "OTHER-001",
    "assetNumber" : "GOV-123456",
    "assignerAccount" : "ASSIGNOR-001",
    "assigneeAccount" : "ASSIGNEE-001",
    "quantity" : "250.75",
    "collateralValue" : {
      "amount" : "24000.00",
      "currency" : "BRL"
    }
  } ]
}