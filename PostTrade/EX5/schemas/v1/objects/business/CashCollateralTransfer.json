{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:CashCollateralTransfer",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:CashCollateralTransfer"
  },
  "title" : "Cash Collateral Transfer",
  "description" : "Cash collateral transfer item with operation value information",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral identification as known by party. Its identification is generated by the requesting party"
    },
    "assetNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identifies the register number of the collateral deposit assigned by the central counterparty"
    },
    "operationValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Amount requested to collateral transfer"
    },
    "collateralValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Collateral accepted value after taking into account the haircut"
    }
  },
  "required" : [ "collateralIdentification", "assetNumber", "operationValue" ],
  "additionalProperties" : true,
  "examples" : [ {
    "collateralIdentification" : "1",
    "assetNumber" : "2015000000349",
    "operationValue" : {
      "amount" : "25000.00",
      "currency" : "BRL"
    },
    "collateralValue" : {
      "amount" : "24000.00",
      "currency" : "BRL"
    }
  } ]
}