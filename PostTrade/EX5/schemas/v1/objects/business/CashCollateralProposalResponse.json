{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:CashCollateralProposalResponse",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:CashCollateralProposalResponse"
  },
  "title" : "Cash Collateral Proposal Response",
  "description" : "Provides details on the cash collateral proposal.",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral identification as known by party. Its identification is generated by the requesting party"
    },
    "assetNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identifies the register number of the collateral deposit assigned by the central counterparty"
    },
    "operationValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Amount requested to collateral deposit."
    },
    "collateralValue" : {
      "$ref" : "../../objects/business/MonetaryValue.json",
      "description" : "Collateral accepted value after taking into account the haircut."
    },
    "statusInformation" : {
      "$ref" : "../../objects/business/CollateralStatus.json",
      "description" : "Overall status of the collateral transfer instruction"
    }
  },
  "required" : [ "collateralIdentification", "assetNumber", "operationValue", "collateralValue", "statusInformation" ],
  "additionalProperties" : false,
  "examples" : [ {
    "collateralIdentification" : "1",
    "assetNumber" : "AAPL",
    "operationValue" : {
      "amount" : "25000.00",
      "currency" : "BRL"
    },
    "collateralValue" : {
      "amount" : "25000.00",
      "currency" : "BRL"
    },
    "statusInformation" : {
      "statusCode" : 1,
      "errorIdentification" : [ {
        "code" : 1,
        "message" : "Error X",
        "description" : "COD12452",
        "refId" : "REF12345"
      } ]
    }
  } ]
}