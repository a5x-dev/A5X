{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:business:SecuritiesCollateralTransfer",
  "$metadata" : {
    "domain" : "business",
    "schemaId" : "urn:a5x:ex5:schema:v1:business:SecuritiesCollateralTransfer"
  },
  "title" : "Securities Collateral Transfer",
  "description" : "Securities collateral transfer item with safekeeping and custody information",
  "type" : "object",
  "properties" : {
    "collateralIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Collateral identification as known by party. Its identification is generated by the requesting party"
    },
    "assetNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identifies the register number of the collateral deposit"
    },
    "quantity" : {
      "$ref" : "../../types/DecimalNumber.json",
      "description" : "Quantity requested to collateral transfer"
    },
    "custodianInformationDestination" : {
      "$ref" : "../../objects/business/CustodianInformation.json",
      "description" : "This block identifies the custodian Information"
    },
    "centralDepositaryOperationNumber" : {
      "type" : "string",
      "pattern" : "^[0-9]{1,9}$",
      "description" : "Central Depository Operation Number"
    }
  },
  "required" : [ "collateralIdentification", "assetNumber", "quantity", "custodianInformationDestination", "centralDepositaryOperationNumber" ],
  "additionalProperties" : true,
  "examples" : [ {
    "collateralIdentification" : "1",
    "assetNumber" : "2015000000349",
    "quantity" : "1000.00",
    "custodianInformationDestination" : {
      "participantInformation" : {
        "id" : 456,
        "category" : 2
      },
      "accountIdentification" : {
        "id" : 789012345678901
      },
      "subAccount" : "001"
    },
    "centralDepositaryOperationNumber" : "123456789"
  } ]
}