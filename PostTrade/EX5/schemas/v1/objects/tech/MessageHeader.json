{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:tech:MessageHeader",
  "$metadata" : {
    "domain" : "tech",
    "schemaId" : "urn:a5x:ex5:schema:v1:tech:MessageHeader"
  },
  "type" : "object",
  "properties" : {
    "Identifier" : {
      "$ref" : "#/$defs/Identifier"
    },
    "MessageType" : {
      "$ref" : "#/$defs/MessageType"
    },
    "Timestamp" : {
      "$ref" : "#/$defs/Timestamp"
    },
    "From" : {
      "$ref" : "#/$defs/From"
    },
    "To" : {
      "$ref" : "#/$defs/To"
    },
    "Sender" : {
      "$ref" : "#/$defs/Sender"
    },
    "Target" : {
      "$ref" : "#/$defs/Target"
    },
    "BeginString" : {
      "$ref" : "#/$defs/BeginString"
    }
  },
  "$defs" : {
    "Identifier" : {
      "$ref" : "../../objects/tech/MessageIdentifier.json"
    },
    "MessageType" : {
      "type" : "string",
      "description" : "Message type in our catalog",
      "examples" : [ "ex5-tec-hdr-0101" ]
    },
    "Timestamp" : {
      "type" : "string",
      "description" : "Trade Date and Time in ISO 8601 format",
      "format" : "date-time",
      "examples" : [ "2026-03-14T00:00:00Z" ],
      "pattern" : "^(([0-9]{4})((-[0-9]{2}){2}(([ T])?([0-9]{2}:){2}[0-9]{2}((\\+[0-9]{2}(:[0-9]{2})?)|Z)?)?|(-W[0-9]{2}(-[1-7])?)|(-[1-3]?[0-9]{2})))$"
    },
    "From" : {
      "$ref" : "../business/PartyId.json"
    },
    "To" : {
      "$ref" : "../business/PartyId.json"
    },
    "Sender" : {
      "$ref" : "../tech/SenderSessionIdentifier.json"
    },
    "Target" : {
      "$ref" : "../tech/TargetSessionIdentifier.json"
    },
    "BeginString" : {
      "type" : "string",
      "description" : "Session",
      "examples" : [ "FIX4.4" ]
    }
  },
  "required" : [ "Identifier", "MessageType", "Timestamp", "From", "To" ],
  "description" : "header Description"
}