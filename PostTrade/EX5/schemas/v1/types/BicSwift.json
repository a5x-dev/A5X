{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:types:BicSwift",
  "$metadata" : {
    "domain" : "types",
    "schemaId" : "urn:a5x:ex5:schema:v1:types:BicSwift"
  },
  "title" : "BIC/SWIFT Code",
  "description" : "Bank Identifier Code (BIC) or SWIFT code validation",
  "type" : "string",
  "pattern" : "^[A-Z]{6}[A-Z0-9]{2}([A-Z0-9]{3})?$",
  "minLength" : 8,
  "maxLength" : 11,
  "examples" : [ "DEUTDEFF", "DEUTDEFF500", "CHASUS33XXX" ]
}