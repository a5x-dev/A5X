{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "urn:a5x:ex5:schema:v1:positions:ex5f-pos-str-0501",
  "$metadata": {
    "domain": "positions",
    "schemaId": "urn:a5x:ex5:schema:v1:positions:ex5f-pos-str-0501"
  },
  "title": "Strategy Breakdown",
  "description": "Details of Strategies",
  "type": "object",
  "properties": {
    "referenceDate": {
      "type": "string",
      "format": "date",
      "description": "Reference date of the information."
    },
    "positionDate": {
      "type": "string",
      "format": "date",
      "description": "Date of reference of position"
    },
    "clearingMemberIdentificationId": {
      "type": "integer",
      "maximum": 999999,
      "minimum": 1,
      "description": "Clearing Member identifier provided by A5X"
    },
    "clearingMemberIdentificationCategory": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999999,
      "description": "Participant category code",
      "$comment": "Only categories (Intermediário Full), (Participante de Liquidação), and (Intermediário Light) are allowed to include an account. Consult External Code List table ExternalParticipantCategoryCode."
    },
    "participantIdentificationId": {
      "type": "integer",
      "maximum": 999999,
      "minimum": 1,
      "description": "Participant identifier provided by A5X"
    },
    "participantIdentificationCategory": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999999,
      "description": "Participant category code",
      "$comment": "Only categories (Intermediário Full), (Participante de Liquidação), and (Intermediário Light) are allowed to include an account. Consult External Code List table ExternalParticipantCategoryCode."
    },
    "accountIdentification": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999999999,
      "description": "Account identifier defined by the participant"
    },
    "tickerSymbol": {
      "type": "string",
      "maxLength": 35,
      "minLength": 1,
      "description": "Letters that identify stocks traded on stock exchange"
    },
    "financialInstrumentIdentificationId": {
      "type": "string",
      "maxLength": 35,
      "minLength": 1,
      "description": "Proprietary identification of an instrument"
    },
    "financialInstrumentIdentificationSource": {
      "type": "string",
      "maxLength": 35,
      "minLength": 1,
      "description": "Source of the instrument identification",
      "default": "8"
    },
    "financialInstrumentIdentificationExchange": {
      "type": "string",
      "pattern": "^[A-Z0-9]{4}$",
      "description": "Identifier code of the exchange where the instrument is listed"
    },
    "isin": {
      "type": "string",
      "maxLength": 12,
      "pattern": "^BR[A-Z0-9]{4}[A-Z0-9]{3}[A-Z0-9]{2}\\d$",
      "description": "ISIN of instrument"
    },
    "market": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999999,
      "description": "Market type - second level of market classification in post-trading process",
      "$comment": "Consult External Code List table ExternalMarketCode"
    },
    "asset": {
      "type": "string",
      "maxLength": 30,
      "description": "Commodity associated with the instrument"
    },
    "tradeDate": {
      "type": "string",
      "format": "date",
      "description": "Strategy Trade Date"
    },
    "tradeIdentification": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35,
      "description": "Strategy Trade identification"
    },
    "allocationIdentification": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35,
      "description": "Identification number of the allocation"
    },
    "dealPrice": {
      "type": "number",
      "multipleOf": 0.00000001,
      "description": "Strategy trade price"
    },
    "buySellIndicator": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999999,
      "description": "Coded list to specify the side of the trade"
    },
    "quantity": {
      "type": "number",
      "multipleOf": 0.00000000000000001,
      "description": "Strategy trade quantity"
    },
    "legNumber": {
      "type": "integer",
      "description": "Strategy Leg Number"
    },
    "legTickerSymbol": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35,
      "description": "Ticker that identifies Leg of a stock traded on a stock exchange"
    },
    "legIdentification": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35,
      "description": "Proprietary identification of an instrument (Strategy Leg)"
    },
    "legSource": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35,
      "description": "Source of the instrument identification (Strategy Leg)"
    },
    "legExchange": {
      "type": "string",
      "pattern": "^[A-Z0-9]{4}$",
      "description": "Code identifying the exchange where the leg instrument was traded"
    },
    "legMarket": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999999,
      "description": "Type of market of the strategy leg"
    },
    "legAsset": {
      "type": "string",
      "maxLength": 30,
      "description": "Commodity associated with the instrument of Strategy Leg"
    },
    "legBuySellIndicator": {
      "type": "integer",
      "minimum": 0,
      "maximum": 999999,
      "description": "Coded list to specify the side of the trade (Side Type Code)"
    },
    "legIsin": {
      "type": "string",
      "maxLength": 12,
      "pattern": "^BR[A-Z0-9]{4}[A-Z0-9]{3}[A-Z0-9]{2}\\d$",
      "description": "ISIN of instrument of Strategy Leg"
    },
    "legDealPrice": {
      "type": "number",
      "multipleOf": 0.00000001,
      "description": "Strategy leg trade price"
    },
    "legQuantity": {
      "type": "number",
      "multipleOf": 0.00000000000000001,
      "description": "Strategy leg trade quantity"
    },
    "legDelta": {
      "type": "number",
      "multipleOf": 0.0000001,
      "description": "Delta value"
    },
    "strategyOfStrategyLegTickerSymbol": {
      "type": "string",
      "minLength": 1,
      "maxLength": 35,
      "description": "Ticker that identifies a stock traded on a stock exchange"
    },
    "strategyOfStrategyLegPrice": {
      "type": "number",
      "multipleOf": 0.00000001,
      "description": "Strategy Leg Price"
    },
    "strategyOfStrategyLegQuantity": {
      "type": "number",
      "multipleOf": 0.00000000000000001,
      "description": "Strategy Leg Quantity"
    }
  },
  "required": [
    "referenceDate",
    "positionDate",
    "asset",
    "clearingMemberIdentificationId",
    "clearingMemberIdentificationCategory",
    "participantIdentificationId",
    "participantIdentificationCategory",
    "accountIdentification",
    "tickerSymbol",
    "financialInstrumentIdentificationId",
    "financialInstrumentIdentificationSource",
    "financialInstrumentIdentificationExchange",
    "market",
    "tradeDate",
    "tradeIdentification",
    "allocationIdentification",
    "dealPrice",
    "buySellIndicator",
    "quantity",
    "legNumber",
    "legIdentification",
    "legSource",
    "legExchange",
    "legMarket",
    "legBuySellIndicator",
    "legDealPrice",
    "legTickerSymbol",
    "legQuantity"
  ],
  "additionalProperties": false
}