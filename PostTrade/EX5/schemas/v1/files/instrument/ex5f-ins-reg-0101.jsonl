  "title": "Financial Instruments Registration File",
  "description": "File for registering multiple types of financial instruments in the A5X system including equities, bonds, derivatives, and other securities",
  "type": "object",
  "properties": {
    "dateTime": {
      "$ref": "../../types/ISODateTime.json",
      "description": "Date and time of file generation"
    },
    "reportNumber": {
      "type": "string",
      "pattern": "^[0-9]{5}$",
      "description": "Sequential number of the report"
    },
    "numberOfMessages": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999999999,
      "description": "Number of messages/instruments within the file"
    },
    "equities": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/Equities.json"
        }
      ],
      "description": "Information specific to equity instruments (stocks)"
    },
    "bdr": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/BDR.json"
        }
      ],
      "description": "Information specific to Brazilian Depositary Receipts"
    },
    "cash": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/Cash.json"
        }
      ],
      "description": "Information specific to cash instruments"
    },
    "adr": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/ADR.json"
        }
      ],
      "description": "Information specific to American Depositary Receipts"
    },
    "internationalBonds": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/InternationalBonds.json"
        }
      ],
      "description": "Information specific to international bond instruments"
    },
    "nationalBonds": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/NationalBonds.json"
        }
      ],
      "description": "Information specific to national bond instruments"
    },
    "etf": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/ETF.json"
        }
      ],
      "description": "Information specific to Exchange Traded Fund instruments"
    },
    "options": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
