          "$ref": "../../objects/business/Future.json"
        }
      ],
      "description": "Information specific to future contract instruments"
    },
    "units": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/Unit.json"
        }
      ],
      "description": "Information specific to unit instruments"
    },
    "etfRendaFixa": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "$ref": "../../objects/business/ETFRendaFixa.json"
        }
      ],
      "description": "Information specific to fixed income ETF instruments"
    },
    "fundoDeLiquidez": {
      "$ref": "../../objects/business/InstrumentCommonFields.json",
      "description": "Information specific to liquidity fund instruments"
    },
    "filax": {
      "allOf": [
        {
          "$ref": "../../objects/business/InstrumentCommonFields.json"
        },
        {
          "properties": {
            "fundName": {
              "type": "string",
              "minLength": 1,
              "maxLength": 150,
              "description": "Fund name"
            }
          }
        }
      ],
      "description": "Information specific to Filax fund instruments"
    }
  },
