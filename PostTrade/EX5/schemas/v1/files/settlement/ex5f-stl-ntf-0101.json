{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "urn:a5x:ex5:schema:v1:settlement:ex5f-stl-ntf-01",
  "$metadata": {
    "domain": "settlement",
    "schemaId": "urn:a5x:ex5:schema:v1:settlement:ex5f-stl-ntf-01"
  },
  "title": "CashEntryType",
  "description": "Cash entry information",
  "type": "object",
  "properties": {
    "referenceDate": {
      "type": "string",
      "format": "date-time",
      "description": "Reference date and time of the information."
    },
    "clearingbrazilianPaymentSystem": {
      "type": "string",
      "description": "Specific clearing identifier for the Brazilian Payment System (SPB) integration",
      "minLength": 1,
      "maxLength": 35
    },
    "clearingIdentification": {
      "type": "integer",
      "minimum": 1,
      "maximum": 999999,
      "description": "Clearing identification"
    },
    "cashEntryTypeCode": {
      "type": "integer",
      "description": "Cash entry type code",
      "minimum": 1,
      "maximum": 999999
    },
    "cashEntryTypeName": {
      "type": "string",
      "description": "Name of cash entry",
      "minLength": 1,
      "maxLength": 60
    },
    "creditDebitIndicator": {
      "type": "string",
      "description": "Indicates whether an entry is a credit or a debit",
      "minLength": 1,
      "maxLength": 35
    },
    "movementTypeCode": {
      "type": "integer",
      "description": "Identifies the code of movement type",
      "minimum": 1,
      "maximum": 999999
    },
    "movementTypeName": {
      "type": "string",
      "minLength": 1,
      "maxLength": 50,
      "description": "The name of the movement type code"
    },
    "reversalIndicator": {
      "type": "boolean",
      "description": "Indicates whether the cash entry type is used to reversal"
    },
    "reversalId": {
      "type": "integer",
      "description": "If the cash entry type has associated reversal, this field will identify the reversal code that should be used for this entry"
    }
  },
  "required": ["referenceDate", "clearingbrazilianPaymentSystem", "clearingIdentification", "cashEntryTypeCode", "cashEntryTypeName", "creditDebitIndicator"],
  "additionalProperties": false
}