{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:Tarifacao:CalculatedFee",
  "$metadata" : {
    "domain" : "Tarifacao",
    "schemaId" : "urn:a5x:ex5:schema:v1:Tarifacao:CalculatedFee"
  },
  "title" : "Calculated Fee",
  "description" : "Calculated Fee Information",
  "type" : "object",
  "properties" : {
    "Date" : {
      "type" : "string",
      "format" : "date",
      "description" : "File Date",
      "examples" : [ "2017-07-21", "2023-12-31", "2024-01-15" ]
    },
    "tradeDate" : {
      "type" : "string",
      "format" : "date",
      "description" : "Trade date",
      "examples" : [ "2017-07-21", "2023-12-31", "2024-01-15" ]
    },
    "ParticipantId" : {
      "type" : "integer",
      "maximum" : 999999,
      "minimum" : 1,
      "description" : "Participant identifier provided by A5X during the participant registration",
      "examples" : [ 114, 85, 123456, 789012 ]
    },
    "ParticipantCategory" : {
      "$ref" : "../../objects/business/ExternalCodeList.json",
      "description" : "Participant category code",
      "$comment" : "Only categories (Intermediário Full), (Participante de Liquidação), and (Intermediário Light) are allowed to include an account. Consult External Code List table ExternalParticipantCategoryCode.",
      "minimum" : 1,
      "maximum" : 99
    },
    "AccountCode" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999999999999,
      "description" : "Account identifier defined by the participant",
      "examples" : [ 987654321012345, 123456789012345 ]
    },
    "documentType" : {
      "$ref" : "../../objects/business/ExternalCodeList.json",
      "description" : "Consult External Code List table ExternalIdentificationTypeCode. IMMUTABLE: Can only be set during creation (OperationType=1). Prohibited in update operations (OperationType=2)"
    },
    "documentNumber" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "pattern" : "^[A-Za-z0-9]+$",
      "description" : "Document number (alphanumeric characters only, no special characters)",
      "examples" : [ "12345678900", "12345678000190", "AB123456", "123456789" ]
    },
    "InvestorName" : {
      "type" : "string",
      "maxLength" : 60,
      "minLength" : 2,
      "pattern" : "^[A-Za-zÀ-ÿ\\s'-]+$",
      "description" : "Complete name of the person",
      "examples" : [ "João Oliveira Santos" ]
    },
    "tickerSymbol" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Letters that identify stocks traded on stock exchange"
    },
    "FinancialInstrumenid" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Proprietary identification of an instrument",
      "examples" : [ "10000000000000001", "10000000000000011" ]
    },
    "secsource" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Source of the instrument identification",
      "default" : "8",
      "examples" : [ "8" ]
    },
    "secexchange" : {
      "type" : "string",
      "pattern" : "^[A-Z0-9]{4}$",
      "description" : "Identifier code of the exchange where the instrument is listed",
      "examples" : [ "A5X", "XNYS", "XNAS" ]
    },
    "FeeComponent" : {
      "$ref" : "../../../schemas_external/ExternalFeeComponents.json",
      "description" : "Consult External Code List table ExternalFeeComponents. "
    },
    "tradeQuantity" : {
      "type" : "string",
      "maxLength" : 20,
      "minLength" : 1,
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "Trade quantity",
      "examples" : [ "1000.50", "25000.75", "999999999999999.99" ]
    },
    "amount" : {
      "type" : "string",
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "Trade amount with up to 2 decimal places",
      "examples" : [ "1000.00", "15750.50", "250000" ]
    },
    "rate" : {
      "type" : "string",
      "maxLength" : 20,
      "minLength" : 1,
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "percentage fee applied",
      "examples" : [ "1000.50", "25000.75", "999999999999999.99" ]
    },
    "Value" : {
      "type" : "string",
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "calculated Fee value",
      "examples" : [ "1000.00", "15750.50", "250000" ]
    }
  },
  "required" : [ "Date", "tradeDate", "ParticipantId", "ParticipantCategory", "AccountCode", "documentType", "documentNumber", "InvestorName", "tickerSymbol", "FinancialInstrumenid", "secsource", "secexchange", "FeeComponent", "rate", "Value" ],
  "additionalProperties" : false,
  "examples" : [ ]
}