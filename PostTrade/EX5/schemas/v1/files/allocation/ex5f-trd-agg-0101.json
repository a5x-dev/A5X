{
  "$schema" : "http://json-schema.org/draft-07/schema#",
  "$id" : "urn:a5x:ex5:schema:v1:allocation:ex5f-trd-agg-01",
  "$metadata" : {
    "domain" : "allocation",
    "schemaId" : "urn:a5x:ex5:schema:v1:allocation:ex5f-trd-agg-01"
  },
  "title" : "CompressedTradesReport",
  "description" : "Compressed Trades Report",
  "type" : "object",
  "properties" : {
    "referenceDate" : {
      "type" : "string",
      "format" : "date",
      "description" : "Reference date of the information"
    },
    "participantIdentificationId" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999,
      "description" : "Identification of the participant taking the allocation, using proprietary scheme"
    },
    "participanteIdentificationCategory" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 99,
      "description" : "Participant category code",
      "$comment" : "Only categories (Intermediário Full), (Participante de Liquidação), and (Intermediário Light) are allowed to include an account. Consult External Code List table ExternalparticipanteIdentificationCategory."
    },
    "accountIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Number account identification"
    },
    "giveUpOnCaptureIndicator" : {
      "type" : "boolean",
      "description" : "Indicates if the  give up was originated due to a trade capture"
    },
    "compressionGroupIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Compression group capture identification"
    },
    "compressionTradeIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Trade identification generated in block capture"
    },
    "previousCompressionTradeIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Identification number of the canceled trade, generated by the block capture"
    },
    "compressionGroupStatus" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999,
      "description" : "Compression group status code",
      "$comment" : "Consult External Code List table ExternalCompressionGroupStatusCode"
    },
    "compressionGroupReason" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999,
      "description" : "Compression group reason code",
      "$comment" : "Consult External Code List table ExternalCompressionGroupReasonCode"
    },
    "tradeDate" : {
      "type" : "string",
      "format" : "date",
      "description" : "Trade date"
    },
    "buySellIndicator" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999,
      "description" : "Indicates whether the trade is a buy or sell",
      "$comment" : "Consult External Code List table ExternalBuySellIndicatorCode"
    },
    "financialInstrumentIdentificationId" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Proprietary identification of an instrument"
    },
    "financialInstrumentIdentificationSource" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Source of the instrument identification"
    },
    "financialInstrumentIdentificationExchange" : {
      "type" : "string",
      "pattern" : "^[A-Z0-9]{4}$",
      "description" : "Identifier code of the exchange where the instrument is listed"
    },
    "compressionTradeQuantity" : {
      "type" : "string",
      "maxLength" : 20,
      "minLength" : 1,
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "Amount of the block capture trade"
    },
    "dealPrice" : {
      "type" : "string",
      "maxLength" : 22,
      "minLength" : 1,
      "pattern" : "^\\d{1,15}(\\.\\d{1,6})?$",
      "description" : "Decimal number with up to 6 decimal places"
    },
    "tickerSymbol" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Letters that identify stocks traded on stock exchange"
    },
    "tradingSessionIdentification" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999,
      "description" : "Indicates if trade was executed during regular hours or after market",
      "$comment" : "Consult External Code List table ExternalTradingSessionIdCode"
    },
    "tradeRegistrationOrigin" : {
      "type" : "string",
      "maxLength" : 50,
      "minLength" : 1,
      "description" : "Origin session code for the trade"
    },
    "blockTraderIdentification" : {
      "type" : "string",
      "maxLength" : 50,
      "minLength" : 1,
      "description" : "Trader code information"
    },
    "blockTradingDeskIdentification" : {
      "type" : "string",
      "maxLength" : 50,
      "minLength" : 1,
      "description" : "Identifies trading desk or strategy"
    },
    "grossTradeAmount" : {
      "type" : "string",
      "maxLength" : 20,
      "minLength" : 1,
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "Gross trade value before any adjustments or deductions"
    },
    "compressionTradeIdDateTime" : {
      "type" : "string",
      "format" : "date-time",
      "description" : "Provides the date and time of creation of the trade generated in the block capture"
    },
    "orderIdentification" : {
      "type" : "string",
      "maxLength" : 26,
      "minLength" : 1,
      "description" : "Order identification number"
    },
    "tradeIdentification" : {
      "type" : "string",
      "maxLength" : 35,
      "minLength" : 1,
      "description" : "Trade identification number"
    },
    "transactionDateTime" : {
      "type" : "string",
      "format" : "date-time",
      "description" : "Transaction date and time"
    },
    "tradeQuantity" : {
      "type" : "string",
      "maxLength" : 20,
      "minLength" : 1,
      "pattern" : "^\\d{1,15}(\\.\\d{1,2})?$",
      "description" : "Trade quantity"
    },
    "traderIdPerTrade" : {
      "type" : "string",
      "maxLength" : 50,
      "minLength" : 1,
      "description" : "Trader code information"
    },
    "tradingDeskIdentification" : {
      "type" : "string",
      "maxLength" : 50,
      "minLength" : 1,
      "description" : "Identifies trading desk or strategy"
    },
    "compressionTradeStatus" : {
      "type" : "integer",
      "minimum" : 1,
      "maximum" : 999999,
      "description" : "Compression trade status code",
      "$comment" : "Consult External Code List table ExternalCompressionTradeStatusCode"
    }
  },
  "required" : [ "participantIdentificationId", "participanteIdentificationCategory", "accountIdentification", "giveUpOnCaptureIndicator", "compressionGroupIdentification", "compressionTradeIdentification", "compressionGroupStatus", "compressionGroupReason", "tradeDate", "buySellIndicator", "financialInstrumentIdentificationId", "financialInstrumentIdentificationSource", "financialInstrumentIdentificationExchange", "compressionTradeQuantity", "dealPrice", "tickerSymbol", "tradingSessionIdentification", "tradeRegistrationOrigin", "grossTradeAmount", "compressionTradeIdDateTime", "orderIdentificationIdent", "tradeIdentificationIdentification", "transactionDateTime", "tradeQuantity", "compressionTradeStatus" ],
  "additionalProperties" : false,
  "examples" : [ ]
}